<p style="color: green"><%= notice %></p>

<div id="api_keys">
  <% requested_api_keys.each do |api_key| %>
    <a class="btn btn-dark btn-sm card-link" data-bs-toggle="collapse" data-bs-target="#api_request_<%= api_key.api_token %>" role="button" aria-expanded="false" aria-controls="api_request_<%= api_key.api_token %>">Show This Request</a>
    <div id="api_request_<%= api_key.api_token %>" class="collapse">
        <hr>
        <%= render api_key %>
        <p>
        <%= link_to "Show This API Key", api_key %>
        </p>
        <%= render partial: "api_keys/accept_form", locals: {api_key: api_key} %>
        <hr>
    </div>
  <% end %>
</div>
